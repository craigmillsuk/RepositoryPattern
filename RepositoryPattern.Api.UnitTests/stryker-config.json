{
  "stryker-config": {
    "mutate": [
      "**/*.cs", // Include ALL .cs files first
      "!**/ResponseModels/**",
      "!**/Middleware/**",
      "!**/Program.cs"
    ],
    "thresholds": {
      "high": 90,
      "low": 85,
      "break": 80
    },
    "reporters": [
      "Progress",
      "Html"
    ]
  }
}